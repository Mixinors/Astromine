{
  "multipart": [
	{   "when": { "facing": "north" },
		"apply": { "model": "astromine:block/express_conveyor" }
	},
	{   "when": { "facing": "east" },
		"apply": { "model": "astromine:block/express_conveyor", "y": 90 }
	},
	{   "when": { "facing": "south" },
		"apply": { "model": "astromine:block/express_conveyor", "y": 180 }
	},
	{   "when": { "facing": "west" },
		"apply": { "model": "astromine:block/express_conveyor", "y": 270 }
	},
	{   "when": { "facing": "north", "left": "false" },
		"apply": { "model": "astromine:block/conveyor_railing" }
	},
	{   "when": { "facing": "east", "left": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 90 }
	},
	{   "when": { "facing": "south", "left": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 180 }
	},
	{   "when": { "facing": "west", "left": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 270 }
	},
	{   "when": { "facing": "north", "right": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 180 }
	},
	{   "when": { "facing": "east", "right": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 270 }
	},
	{   "when": { "facing": "south", "right": "false" },
		"apply": { "model": "astromine:block/conveyor_railing" }
	},
	{   "when": { "facing": "west", "right": "false" },
		"apply": { "model": "astromine:block/conveyor_railing", "y": 90 }
	},
	{   "when": { "up": "true" },
		"apply": { "model": "astromine:block/conveyor_pillars" }
	}
  ]
}