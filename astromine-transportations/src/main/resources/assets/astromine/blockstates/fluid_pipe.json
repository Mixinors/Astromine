{
  "multipart": [
    {
      "apply": {
        "model": "astromine:block/fluid_pipe_center"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side",
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side",
        "x": 270
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "astromine:block/fluid_pipe_side",
        "x": 90
      }
    }
  ]
}
